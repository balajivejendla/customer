{
  "name": "backend",
  "version": "1.0.0",
  "description": "JWT Authentication Backend with WebSocket support",
  "main": "server.js",
  "scripts": {
    "start": "node start-production.js",
    "start:dev": "node server.js",
    "start:socket": "node sockets.js",
    "dev": "nodemon server.js",
    "socket": "node sockets.js",
    "test": "node test-websocket.js",
    "test-auth": "echo \"Testing JWT authentication...\" && curl -X POST http://localhost:4000/auth/login -H \"Content-Type: application/json\" -d '{\"email\":\"test@example.com\",\"password\":\"password123\"}'",
    "test-websocket": "node test-websocket.js",
    "test-registration": "node test-registration.js",
    "seed-faq": "node seed-faq.js",
    "seed-faq-sample": "node seed-faq.js --sample",
    "seed-faq-force": "node seed-faq.js --force",
    "test-rag": "node -e \"const rag = require('./rag.service'); rag.testPipeline().then(console.log).catch(console.error);\"",
    "test-queue": "node test-queue-system.js",
    "setup": "node quick-setup.js",
    "build": "echo 'No build step required for Node.js'",
    "docker:build": "docker build -t backend-server .",
    "docker:run": "docker run -p 4000:4000 -p 3000:3000 --env-file .env backend-server",
    "docker:test": "node test-docker.js"
  },
  "keywords": [
    "jwt",
    "authentication",
    "websocket",
    "express",
    "socket.io"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "bull-board": "^2.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "redis": "^4.6.12",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "redis-server": "^1.2.2"
  }
}
